{
  "name": "astm-parser",
  "version": "1.0.0",
  "description": "Parser for ASTM format",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha --timeout 10000 -r ts-node/register test/**/*.ts",
    "test:watch": "mocha --timeout 10000 --watch -r ts-node/register test/**/*.ts",
    "format": "prettier --write --parser typescript **/*.ts",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "keywords": [
    "ASTM",
    "AAMA",
    "DXF",
    "Apparel"
  ],

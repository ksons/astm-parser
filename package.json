    "d3-scale-chromatic": "^3.1.0",
    "dxf-parser": "^1.1.2",
    "glob": "^13.0.0",
    "lodash": "^4.17.21",
    "pretty-data": "^0.40.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "ajv": "^8.17.1",
    "chai": "^6.2.2",
    "foreach-in-dir": "^1.2.1",
    "json-schema-to-typescript": "^15.0.4",
    "mocha": "^11.7.5",
    "prettier-standard": "^16.4.1",
    "rename-extension": "^4.0.0",
    "safename": "^2.0.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^5.9.3"

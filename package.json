{
  "name": "astm-parser",
  "version": "1.0.0",
  "description": "Parser for ASTM format",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "tsx node_modules/mocha/bin/mocha.js --timeout 10000 test/**/*.ts",
    "test:watch": "tsx node_modules/mocha/bin/mocha.js --timeout 10000 --watch test/**/*.ts",
    "lint": "eslint src test",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": [
    "ASTM",
    "AAMA",
    "DXF",
    "Apparel"
  ],
  "author": "Kristian Sons",
  "license": "ISC",
  "dependencies": {
    "@types/d3": "^7.4.3",
    "@types/lodash": "^4.17.21",
    "d3": "^7.9.0",
    "d3-scale-chromatic": "^3.1.0",
    "dxf-parser": "^1.1.2",
    "glob": "^13.0.0",
    "lodash": "^4.17.21",
    "pretty-data": "^0.40.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "ajv": "^8.17.1",
    "chai": "^6.2.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "foreach-in-dir": "^1.2.1",
    "json-schema-to-typescript": "^15.0.4",
    "mocha": "^11.7.5",
    "prettier": "^3.7.4",
    "rename-extension": "^4.0.0",
    "safename": "^2.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  }
}
